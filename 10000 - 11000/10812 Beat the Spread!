#include <iostream>
using namespace std;

int main() {
    int t;
    int s;
    int d;
    int a;
    int b;

    cin >> t;

    for(int i=0; i<t; i++) {
        cin >> s;
        cin >> d;
        
        if((s + d)%2 == 1) a = -1;
        else a = (s + d) / 2;
        
        if((s - d)%2 == 1) b = -1;
        else b = (s - d) / 2;
        
        if(a >= 0 && b >= 0) cout << a << " " << b << endl;
        else cout << "impossible" << endl;
    }
    return 0;
}


