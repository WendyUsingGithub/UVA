#include <iostream>
#include <string.h>
using namespace std;

int main() {

    char s1[1000];
    char s2[1000];
    char count1[26];
    char count2[26];
    char count;
    char a = 'a';

    memset(count1, 0, sizeof(count1));
    memset(count2, 0, sizeof(count2));

    while(cin>>s1) {
        cin>>s2;
        for(int i=0; i<strlen(s1); i++) {
            count1[s1[i]-a]++;
        }
        for(int i=0; i<strlen(s2); i++) {
            count2[s2[i]-a]++;
        }
        for(int i=0; i<26; i++) {
            count = (count1[i] - count2[i])>0 ? count2[i] : count1[i];
            for(int j=0; j<count; j++) {
                cout<<(char)(i+a);
            }
        }
        cout<<endl;
    }
    return 0;
}

