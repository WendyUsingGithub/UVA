#include <stdlib.h>
#include <iostream>
#include <string>
using namespace std;

int main() {

    string s;
    int len;
    int count;
    int flag = 0;
    int N;
    char c;

    while(1) {

        getline(cin, s);
        len = s.length();
        if(s == ".") break;

        for(int n=1; n<=len; n++) {
          
            if(len % n != 0) continue; /* this length does not even divide */

            for(int init=0; init<n; init++) {
                
                c = s[init];
                count = 1;
                flag = 0;

                while(1) {

                    if(init + count * n >= len) break; /* out off boundry */
                    if(c != s[init + count * n]) {
                        flag = 1;
                        break;
                    }
                    count++;
                }
            }

            if(flag == 0) {
                N = n;
                n = len + 1;
                break;
            }
            
        }

        if(flag == 0) cout << len / N << endl;
        else cout << "1" << endl;
    }
}
