// 據網路上說測資裡面會有正號負號空格等 不是數字或英文字母的字元出現
// 所以輸入之後要檢查是不是數字或英文字母

#include <iostream>
#include <string>
#include <cctype>
#include <algorithm>
using namespace std;

int main(){

    string symbol = "0123456789" "ABCDEFGHIJKLMNOPQRSTUVWXYZ" "abcdefghijklmnopqrstuvwxyz";
    string num;

    while(cin >> num){
        int len = num.length(); 
        int* r = (int*)malloc(len* sizeof(int));

        for(int i=0; i<len; i++) {
            r[i] = symbol.find(num[i]);

            if(r[i] < 0 || r[i] > 62){
                r[i] = 0;
            }
        }

        int n = *max_element(r, r+len) + 1;
        n = max(n, 2);

        for( ; n<=62; n++) {
            int rsd = 0;
            for (int i=0; i<len; i++) {
                rsd = rsd * n + r[i];
                rsd = rsd % (n - 1);
            }
            if(rsd == 0) break;
        }
        if(n <= 62) cout << n;
        else cout << "such number is impossible!";
        cout << endl;
    }   
    return 0;
}
